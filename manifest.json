{
  "manifest_version": 3,
  "name": "jpdb.io tweaks",
  "version": "0.1.0",
  "author": "ElnuDev",
  "homepage_url": "https://github.com/ElnuDev/jpdb-tweaks",
  "description": "Simple tweaks for jpdb.io, a Japanese dictionary and spaced repetition system. Adds copy markdown button to example sentences.",
  "content_scripts": [
    {
      "matches": ["https://jpdb.io/*"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "16": "favicon16.png",
    "32": "favicon32.png",
    "48": "favicon48.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "jpdb-tweaks@elnu.com"
    }
  },
  "permissions": [
  	"storage"
  ],
  "options_page": "options.html"
}
